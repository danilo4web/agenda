    "components": {
        "securitySchemes": {
            "bearerToken": {
                "type": "http",
                "description": "Token de acesso obtido na autenticação",
                "name": "Autorização",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
